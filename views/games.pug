  -let results = [{ name: 'Call of Duty', style: "FPS", rating: 4.5, size: 258 },{ name: 'Injustice 2', style: "Fighting", rating: 3.4, size: 96 },{ name: 'League of Legends', style: "MOBA", rating: 4.7, size: 23 }];

  div(class="container-fluid")
